{
  "name": "bernheisel.com",
  "description": "David Bernheisel's Blog",
  "version": "1.0.0",
  "author": "David Bernheisel <david@bernheisel.com>",
  "bugs": {
    "url": "https://github.com/dbernheisel/dbernheisel.github.io/issues"
  },
  "scripts": {
    "dev": "yarn run build:css && yarn run gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "build": "yarn run gatsby clean && yarn run build:css && gatsby build",
    "build:serve": "yarn run build && yarn run gatsby serve",
    "build:css": "tailwind build src/css/tailwind.src.css -c src/css/tailwind.config.js -o src/css/tailwind.css",
    "deploy": "yarn run build:css && gatsby build && gh-pages -b master -d public"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.0.0",
    "gatsby-image": "^2.0.0",
    "gatsby-plugin-feed": "^2.0.0",
    "gatsby-plugin-google-analytics": "^2.0.0",
    "gatsby-plugin-manifest": "^2.0.0",
    "gatsby-plugin-offline": "^3.0.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.0",
    "gatsby-plugin-sitemap": "^2.2.28",
    "gatsby-plugin-twitter": "^2.0.0",
    "gatsby-remark-audio": "^1.0.2",
    "gatsby-remark-copy-linked-files": "^2.0.0",
    "gatsby-remark-images": "^3.0.0",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.0",
    "gatsby-remark-smartypants": "^2.0.0",
    "gatsby-remark-video": "^1.2.5",
    "gatsby-source-filesystem": "^2.0.0",
    "gatsby-transformer-remark": "^2.0.0",
    "gatsby-transformer-sharp": "^2.0.0",
    "lodash": "^4.17.10",
    "prismjs": "^1.15.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-react": "^7.7.0",
    "gatsby-plugin-postcss": "^2.0.0",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gh-pages": "^2.0.0",
    "precss": "^3.1.2",
    "prettier": "^2.0.0",
    "tailwindcss": "^1.0.0"
  },
  "homepage": "https://github.com/dbernheisel/dbernheisel.github.io",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbernheisel/dbernheisel.github.io.git"
  }
}
